<div class="pagination">
	<div class="pagination__inner">
		<div class="pagination__box">
			{% for item in[
				1,
				2,
				3,
				4,
				5,
				6,
				7
			] %}
				<a href="#" class="pagination__item">
					<div class="pagination__text">{{item}}</div>
				</a>
			{% endfor %}
		</div>

	</div>
</div>